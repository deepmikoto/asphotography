{% spaceless %}
    <!DOCTYPE html>
    <html lang="eng">
    <head>
        {% stylesheets
            'bundles/photography/css/*'
            'bundles/photography/scss/asphotography.scss'
            'bundles/photography/scss/media_queries/*'
            output='css/asphotography.css' filter='cssrewrite'
        %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />
        {% endstylesheets %}
        {% image '@PhotographyBundle/Resources/public/images/eos-350d_16.png' output='images/favicon.png' %}
            <link rel="icon" type="image/png" href="{{ asset_url }}">
        {% endimage %}
        <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0">
        <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed:400,700' rel='stylesheet' type='text/css'>
        {% block meta_title %}
            <title>{{ site_title }}</title>
        {% endblock %}
    </head>
    <body>
    {% block body %}{% endblock %}
    <script type="text/javascript">var root = '{{ path('asphotography_app_homepage') }}';</script>
    {% javascripts
        '@PhotographyBundle/Resources/public/js/vendor/jquery-2.1.4.js'
        '@PhotographyBundle/Resources/public/js/vendor/json2.js'
        '@PhotographyBundle/Resources/public/js/vendor/underscore.js'
        '@PhotographyBundle/Resources/public/js/vendor/backbone.js'
        '@PhotographyBundle/Resources/public/js/vendor/backbone.marionette.js'
        '@PhotographyBundle/Resources/public/js/vendor/backbone.wreqr.js'
        '@PhotographyBundle/Resources/public/js/vendor/js.cookie.js'
        '@PhotographyBundle/Resources/public/js/app.js'
        '@PhotographyBundle/Resources/public/js/app/models/*'
        '@PhotographyBundle/Resources/public/js/app/collections/*'
        '@PhotographyBundle/Resources/public/js/app/views/*'
        '@PhotographyBundle/Resources/public/js/app/functions/*'
        '@PhotographyBundle/Resources/public/js/app/_router.js'
        '@PhotographyBundle/Resources/public/js/app/asphotography.js'
        output='js/asphotography.js' filter='yui_js'
    %}
    <script type="text/javascript" src="{{ asset_url }}" ></script>
    {% endjavascripts %}
    </body>
    </html>
{% endspaceless %}
